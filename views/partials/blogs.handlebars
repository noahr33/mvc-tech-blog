<div>
  {{#each blogs}}
    <div class="card bg-primary m-3 p-3 border border-light border-end-0 rounded-start">
      <h2>
        <a href="/blog-post/{{id}}" class="text-warning icon-link icon-link-hover">{{blog_title}}</a>
      </h2>
      <div class="text-light">
        Posted by {{user.username}} on {{format_date date_created}}.
      </div>
    </div>
  {{/each}}
</div>

    
